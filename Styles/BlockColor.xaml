<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>

        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="EventBlockColorBrush" Color="#FFAA00"/>
            <SolidColorBrush x:Key="ControlBlockColorBrush" Color="#FFC800"/>
            <SolidColorBrush x:Key="TextValueBlockColorBrush" Color="#962020"/>
            <SolidColorBrush x:Key="NumberValueBlockColorBrush" Color="#0060FF"/>
            <SolidColorBrush x:Key="BooleanValueBlockColorBrush" Color="#78FF32"/>
        </ResourceDictionary>

    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
